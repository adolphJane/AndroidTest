        <attr name="cvRoundAngle" format="dimension|reference"/>
        <attr name="cvStrokeWidth" format="dimension|reference"/>
        <attr name="cvStrokeColor" format="color|reference"/>
        <attr name="cvTextContent" format="string|reference"/>
        <attr name="cvTextSize" format="dimension|reference"/>
